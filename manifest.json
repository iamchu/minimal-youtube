{
  "manifest_version": 2,
 
  "name": "Minimal-Youtube",
  "version": "0.1.0",
  "description": "Remove youtube addiction inducing features.",
 
  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["content.js"],
    "matches": ["https://www.youtube.com/watch*", "https://www.youtube.com/result*"]
  }]
}
